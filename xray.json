{
    "log": {
        "loglevel": "warning"
    },
    "dns": {
        "hosts": {
            "domain:googleapis.cn": "googleapis.com"
        },
        "servers": [
            "1.1.1.1"
        ]
    },
    "inbounds": [
        {
            "listen": "127.0.0.1",
            "port": 10808,
            "protocol": "socks",
            "settings": {
                "auth": "noauth",
                "udp": true,
                "userLevel": 8
            },
            "sniffing": {
                "destOverride": [
                    "http",
                    "tls"
                ],
                "enabled": true
            },
            "tag": "socks"
        }
    ],
    "outbounds": [
        {
            "protocol": "wireguard",
            "settings": {
                "address": [
                    "172.16.0.2/32",
                    "2606:4700:110:8735:bb29:91bc:1c82:aa73/128"
                ],
                "mtu": 1280,
                "peers": [
                    {
                        "endpoint": "162.159.195.231:7156",
                        "publicKey": "bmXOC+F1FxEMF9dyiK2H5/1SUtzH0JuVo51h2wPfgyo="
                    }
                ],
                "reserved": [
                    142,
                    62,
                    250
                ],
                "secretKey": "2KeURdY/DCcCBYpveLZX+BaOqbFo4i5Cn8giXqqumnk="
            },
            "streamSettings": {
                "sockopt": {
                    "dialerProxy": "noise"
                }
            },
            "tag": "proxy"
        },
        {
            "protocol": "freedom",
            "settings": {
                "noises": [
                    {
                        "type": "base64",
                        "packet": "7nQBAAABAAAAAAAABnQtcmluZwZtc2VkZ2UDbmV0AAABAAE=",
                        "delay": "30-60"
                    }
                ]
            },
            "tag": "noise"
        },
        {
            "protocol": "freedom",
            "settings": {},
            "tag": "direct"
        },
        {
            "protocol": "blackhole",
            "settings": {
                "response": {
                    "type": "http"
                }
            },
            "tag": "block"
        }
    ],
    "routing": {
        "domainStrategy": "IPIfNonMatch",
        "rules": [
            {
                "ip": [
                    "1.1.1.1"
                ],
                "outboundTag": "proxy",
                "port": "53",
                "type": "field"
            },
            {
                "port": "53",
                "outboundTag": "dns-out",
                "type": "field"
            },
            {
                "port": "853",
                "network": "tcp",
                "outboundTag": "block"
            },
            {
                "outboundTag": "block",
                "network": "udp",
                "port": "853, 443"
            },
            {
                "domain": [
                    "domain:.ir",
                    "geosite:category-ir"
                ],
                "outboundTag": "direct",
                "type": "field"
            },
            {
                "ip": [
                    "geoip:ir",
                    "geoip:private"
                ],
                "outboundTag": "direct",
                "type": "field"
            },
            {
                "type": "field",
                "port": "0-65535",
                "outboundTag": "proxy",
                "enabled": true
            }
        ]
    },
    "remarks": "WarpNoise"
}
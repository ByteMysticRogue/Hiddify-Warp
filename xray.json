{
    "log": {
        "loglevel": "warning"
    },
    "dns": {
        "hosts": {
            "geosite:category-ads-all": [
                "127.0.0.1"
            ],
            "geosite:category-ads-ir": [
                "127.0.0.1"
            ]
        },
        "servers": [
            "1.1.1.1",
            "1.0.0.1",
            {
                "address": "8.8.8.8",
                "domains": [
                    "geosite:category-ir"
                ],
                "expectIPs": [
                    "geoip:ir"
                ],
                "skipFallback": true
            }
        ],
        "queryStrategy": "UseIPv4",
        "tag": "dns"
    },
    "inbounds": [
        {
            "port": 10808,
            "protocol": "socks",
            "settings": {
                "auth": "noauth",
                "udp": true,
                "userLevel": 8
            },
            "sniffing": {
                "destOverride": [
                    "http",
                    "tls"
                ],
                "enabled": true,
                "routeOnly": true
            },
            "tag": "socks-in"
        },
        {
            "port": 10809,
            "protocol": "http",
            "settings": {
                "auth": "noauth",
                "udp": true,
                "userLevel": 8
            },
            "sniffing": {
                "destOverride": [
                    "http",
                    "tls"
                ],
                "enabled": true,
                "routeOnly": true
            },
            "tag": "http-in"
        },
        {
            "listen": "127.0.0.1",
            "port": 10853,
            "protocol": "dokodemo-door",
            "settings": {
                "address": "1.1.1.1",
                "network": "tcp,udp",
                "port": 53
            },
            "tag": "dns-in"
        }
    ],
    "outbounds": [
        {
            "protocol": "wireguard",
            "settings": {
                "address": [
                    "172.16.0.2/32",
                    "2606:4700:110:8735:bb29:91bc:1c82:aa73/128"
                ],
                "mtu": 1280,
                "peers": [
                    {
                        "endpoint": "162.159.195.111:939",
                        "publicKey": "bmXOC+F1FxEMF9dyiK2H5/1SUtzH0JuVo51h2wPfgyo="
                    }
                ],
                "reserved": [
                    77,
                    211,
                    121
                ],
                "secretKey": "qJJQOjhmomJH9vA0tb5LGsXeNom2+DJQza/yrqTcwXI="
            },
            "streamSettings": {
                "sockopt": {
                    "dialerProxy": "noise"
                }
            },
            "tag": "proxy"
        },
        {
            "protocol": "freedom",
            "settings": {
                "noises": [
                    {
                        "type": "base64",
                        "packet": "7nQBAAABAAAAAAAABnQtcmluZwZtc2VkZ2UDbmV0AAABAAE=",
                        "delay": "30-60"
                    }
                ]
            },
            "tag": "noise"
        },
        {
            "protocol": "freedom",
            "settings": {
                "domainStrategy": "UseIPv4"
            },
            "tag": "direct"
        },
        {
            "protocol": "blackhole",
            "settings": {
                "response": {
                    "type": "http"
                }
            },
            "tag": "block"
        }
    ],
    "policy": {
        "levels": {
            "8": {
                "connIdle": 300,
                "downlinkOnly": 1,
                "handshake": 4,
                "uplinkOnly": 1
            }
        },
        "system": {
            "statsOutboundUplink": true,
            "statsOutboundDownlink": true
        }
    },
    "routing": {
        "domainStrategy": "IPIfNonMatch",
        "rules": [
            {
                "inboundTag": [
                    "dns-in"
                ],
                "outboundTag": "dns-out",
                "type": "field"
            },
            {
                "inboundTag": [
                    "socks-in",
                    "http-in"
                ],
                "port": "53",
                "outboundTag": "dns-out",
                "type": "field"
            },
            {
                "ip": [
                    "8.8.8.8"
                ],
                "port": "53",
                "network": "udp",
                "outboundTag": "direct",
                "type": "field"
            },
            {
                "port": "853",
                "network": "tcp",
                "outboundTag": "block"
            },
            {
                "outboundTag": "block",
                "network": "udp",
                "port": "853, 443"
            },
            {
                "domain": [
                    "geosite:category-ads-all"
                ],
                "outboundTag": "block",
                "type": "field"
            },
            {
                "domain": [
                    "domain:.ir",
                    "geosite:category-ir"
                ],
                "outboundTag": "direct",
                "type": "field"
            },
            {
                "ip": [
                    "geoip:ir",
                    "geoip:private"
                ],
                "outboundTag": "direct",
                "type": "field"
            },
            {
                "network": "tcp,udp",
                "outboundTag": "proxy",
                "type": "field"
            }
        ]
    },
    "remarks": "WarpNoise"
}